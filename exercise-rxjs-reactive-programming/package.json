{
  "name": "exercise-rxjs-reactive-programing",
  "version": "0.0.0",
  "scripts": {
    "start": "npm-run-all --parallel serve serve:api",
    "serve": "ng serve -o --port=4302",
    "serve:api": "json-server --watch db.json --delay 250 --port 4301 --routes db-routes.json",
    "build": "ng build --prod",
    "test": "npm run format:test && ng lint && ng test --watch false",
    "watch": "ng test --reporters spec",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:write": "prettier projects/**/*.{js,ts,json,html,scss,md} --write",
    "format:test": "prettier projects/**/*.{js,ts,json,html,scss,md} --list-different",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/customer-admin-app/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.8",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.8",
    "@angular/compiler": "~8.2.8",
    "@angular/core": "~8.2.8",
    "@angular/forms": "~8.2.8",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.8",
    "@angular/platform-browser-dynamic": "~8.2.8",
    "@angular/router": "~8.2.8",
    "@types/jest": "^24.0.19",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "uuid": "^3.3.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.6",
    "@angular/cli": "~8.3.6",
    "@angular/compiler-cli": "~8.2.8",
    "@angular/language-service": "~8.2.8",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.15.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-spec-reporter": "0.0.32",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
